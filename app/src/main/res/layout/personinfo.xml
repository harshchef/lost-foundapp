<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:background="#FF000000"
    android:layout_height="match_parent">


    <ImageView
        android:id="@+id/personal"
        android:layout_width="401dp"
        android:layout_height="176dp"
        android:layout_marginTop="4dp"
        android:textColor="#1AD121"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        android:src="@drawable/ppo"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/pegv"

        android:layout_width="match_parent"
        android:layout_height="56dp"
        android:layout_marginTop="608dp"
        android:backgroundTint="#009688"
        android:textColor="#1AD121"
        android:enabled="false"
        android:ems="10"
        android:inputType="none"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="TouchTargetSizeCheck,TouchTargetSizeCheck,SpeakableTextPresentCheck" />

    <EditText
        android:id="@+id/personfamily"
        android:layout_width="match_parent"
        android:layout_height="56dp"
        android:layout_marginTop="484dp"
        android:ems="10"
        android:inputType="none"
        android:enabled="false"
        android:textColor="#1AD121"
        app:layout_constraintEnd_toEndOf="parent"
        android:backgroundTint="#009688"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="TouchTargetSizeCheck,TouchTargetSizeCheck,SpeakableTextPresentCheck,SpeakableTextPresentCheck" />

    <EditText
        android:id="@+id/persongivenname"
        android:layout_width="match_parent"
        android:layout_height="70dp"
        android:layout_marginTop="36dp"
        android:backgroundTint="#009688"
        android:textColor="#1AD121"
        android:ems="10"
        android:inputType="none"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        android:enabled="false"
        app:layout_constraintTop_toBottomOf="@+id/namee"
        tools:ignore="TouchTargetSizeCheck,TouchTargetSizeCheck,SpeakableTextPresentCheck,SpeakableTextPresentCheck" />

    <EditText
        android:id="@+id/namee"
        android:layout_width="402dp"
        android:backgroundTint="#009688"
        android:layout_height="88dp"
        android:layout_marginTop="216dp"
        android:textColor="#1AD121"
        android:ems="10"
        android:enabled="false"
        android:inputType="none"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/personal"
        tools:ignore="TouchTargetSizeCheck,TouchTargetSizeCheck,SpeakableTextPresentCheck,SpeakableTextPresentCheck" />

</androidx.constraintlayout.widget.ConstraintLayout>